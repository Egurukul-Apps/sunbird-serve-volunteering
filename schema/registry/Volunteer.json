{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "Volunteer": {
      "$ref": "#/definitions/Volunteer"
    }
  },
  "required": [
    "Volunteer"
  ],
  "title": "Volunteer",
  "definitions": {
    "Volunteer": {
      "$id": "#/properties/Volunteer",
      "type": "object",
      "title": "The Volunteer Schema",
      "required": [],
      "properties": {
        "identityDetails": {
          "$ref": "#/definitions/IdentityDetails"
        },
        "contactDetails": {
          "$ref": "#/definitions/ContactDetails"
        },
        "volunteerProfileId": {
          "type": "string"
        },
         "status": {
          "type": "string"
        }
      }
    },
    "IdentityDetails": {
      "$id": "#/definitions/IdentityDetails",
      "type": "object",
      "title": "Identity Details",
      "description": "Identity Details",
      "required": [],
      "properties": {
        "fullname": {
          "type": "string"
        },
        "gender": {
          "type": "string",
          "enum": [
            "Male",
            "Female",
            "Other"
          ],
          "title": "Gender"
        },
        "dob": {
          "type": "string",
          "format": "date"
        },
        "Nationality": {
          "type": "string"
        }
      }
    },
    "ContactDetails": {
      "$id": "#/definitions/ContactDetails",
      "type": "object",
      "title": "Contact Details",
      "description": "Contact Details",
      "required": [],
      "properties": {
        "email": {
          "type": "string",
          "title": "Email"
        },
        "mobile": {
          "type": "string",
          "title": "Mobile"
        },
        "address": {
          "$ref": "#/definitions/Address",
          "title": "Address"
        }
      }
    },
    "Address": {
      "type": "object",
      "required": [],
      "additionalProperties": false,
      "properties": {
        "plot": {
          "type": "string",
          "title": "Plot"
        },
        "street": {
          "type": "string",
          "title": "Street"
        },
        "landmark": {
          "type": "string",
          "title": "Landmark"
        },
        "locality": {
          "type": "string",
          "title": "Locality"
        },
        "state": {
          "type": "string",
          "title": "State",
          "enum": [
            "Andaman and Nicobar Islands",
            "Andhra Pradesh",
            "Arunachal Pradesh",
            "Assam",
            "Bihar",
            "Chandigarh",
            "Chhattisgarh",
            "Dadra and Nagar Haveli",
            "Daman and Diu",
            "Delhi",
            "Goa",
            "Gujarat",
            "Haryana",
            "Himachal Pradesh",
            "Jammu and Kashmir",
            "Jharkhand",
            "Karnataka",
            "Kerala",
            "Ladakh",
            "Lakshadweep",
            "Madhya Pradesh",
            "Maharashtra",
            "Manipur",
            "Meghalaya",
            "Mizoram",
            "Nagaland",
            "Odisha",
            "Puducherry",
            "Punjab",
            "Rajasthan",
            "Sikkim",
            "Tamil Nadu",
            "Telangana",
            "Tripura",
            "Uttar Pradesh",
            "Uttarakhand",
            "West Bengal"
          ]
        },
        "district": {
          "type": "string",
          "title": "District"
        },
        "village": {
          "type": "string",
          "title": "Village/Town/City"
        },
        "pincode": {
          "type": "string",
          "title": "Pincode"
        }
      }
    }
  },
  "_osConfig": {
    "privateFields": [
      "$.identityDetails.dob",
      "$.identityDetails.identityHolder.type",
      "$.identityDetails.identityHolder.value",
      "$.contactDetails.email",
      "$.contactDetails.mobile",
      "$.contactDetails.address.plot",
      "$.contactDetails.address.street",
      "$.contactDetails.address.landmark",
      "$.contactDetails.address.locality"
    ],
    "internalFields": [],
    "indexFields": [],
    "uniqueIndexFields": [],
    "systemFields": [
      "osCreatedAt",
      "osUpdatedAt",
      "osCreatedBy",
      "osUpdatedBy"
    ],
    "inviteRoles": [
      "anonymous"
    ],
    "ownershipAttributes": [
      {
        "email": "/contactDetails/email",
        "mobile": "/contactDetails/mobile",
        "userId": "/contactDetails/mobile"
      }
    ]
  }
}